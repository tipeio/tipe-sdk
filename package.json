{
  "name": "tipe.js",
  "version": "0.0.3",
  "description": "Tipe client for JavaScript and Node.js",
  "browser": "dist/index.js",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "typings": "dist/_types/index.d.ts",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc --module commonjs",
    "test": "jest --verbose false",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest",
    "ci": "yarn lint && yarn test --coverage --no-cache",
    "preversion": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tipeio/tipe-client.git"
  },
  "contributors": [
    "PatrickJS <patrick@tipe.io>",
    "Scott Moss <scott@tipe.io>",
    "Mike Moss <mike@tipe.io>"
  ],
  "engines": {
    "node": ">=8"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/tipeio/tipe-client/issues"
  },
  "homepage": "https://github.com/tipeio/tipe-client#readme",
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.7",
    "@vue/eslint-config-standard": "^3.0.0-beta.10",
    "jest": "^23.6.0",
    "regenerator-runtime": "^0.11.1",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "@types/node-fetch": "^2.1.4",
    "fast-json-stable-stringify": "^2.0.0",
    "node-fetch": "^2.3.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "/dist/"
    ],
    "testPathIgnorePatterns": [
      "/dist/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.{js,ts}",
      "!src/**/*.d.ts"
    ]
  }
}
